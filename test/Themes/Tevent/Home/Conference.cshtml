@model Conference
@{
    ViewData["Title"] = Model.Title;

    var diff = Model.FromDateTime - DateTime.Now;
}

<!--hero section start-->
<section data-overlay="5" style="height: 531px;">
    <div class="ImageBackground__holder" style="background-image: url('/theme/tevent/images/ban2.jpg'); background-position: 50% -327px;">
        <img src="/theme/tevent/images/ban2.jpg" alt="">
    </div>
    <div class="container u-vCenter">
        <div class="row text-center text-left- text-white">
            <div class="col-md-12">
                <h2 class="u-FontSize75 u-xs-FontSize40 u-Weight700 u-MarginTop150 u-MarginBottom0">International Conference 2017</h2>
                <h3 class="text-uppercase  text-white u-Weight700 u-MarginTop0 u-MarginBottom50">15 - 19 July, 2017, San Francisco, CA </h3>
                <a class="btn btn-gradient- btn--alien- btn-primary text-uppercase u-MarginBottom10 u-LetterSpacing2" href="#">Get Tickets</a>

                <div id="counting-date" class="count-transparent- h3 u-MarginTop50 u-MarginBottom0 u-xs-MarginTop30"></div>

            </div>
        </div>
    </div>
</section>
<!--hero section end-->
<!--about start-->
<section class=" u-PaddingTop100 u-PaddingBottom100  u-xs-PaddingBottom30 u-xs-PaddingTop30 u-sm-PaddingTop50 u-MarginBottom0 u-zIndex10 position-relative">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2 col-sm-12">
                <h1 class="u-MarginTop0 u-sm-MarginTop30 u-Weight700">درباره همایش</h1>
                <p class=""> @Html.Raw(Model.Description)</p>

                <div class="row">
                    <div class="col-sm-6 u-MarginTop50">
                        <p><i class="Icon Icon-map Icon--32px text-primary" aria-hidden="true"></i></p>
                        <h4>مکان</h4>
                        <p>
                            West International Conference Center
                        </p>
                        <p>@Model.CityName</p>
                    </div>
                    <div class="col-sm-6 u-MarginTop50">
                        <p><i class="Icon Icon-clock Icon--32px text-primary" aria-hidden="true"></i></p>
                        <h4>زمان</h4>
                        <p>
                            Saturday to Thursday (09:00 AM to 05:00 PM)
                        </p>
                        <p>@Model.FromDateTime.ToPersian(Model.ToDateTime) </p>
                    </div>
                </div>

                <div class="u-MarginTop50">
                    <a class="btn btn-primary text-uppercase u-MarginBottom10 u-LetterSpacing2" href="#">Learn More</a>
                </div>
            </div>
        </div>
    </div>
</section>
