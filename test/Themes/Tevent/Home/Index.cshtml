
<div class="row">
    <div class="col-xs-12">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="/images/slideshow/la.jpg" alt="Los Angeles">
                </div>

                <div class="item">
                    <img src="/images/slideshow/chicago.jpg" alt="Chicago">
                </div>

                <div class="item">
                    <img src="/images/slideshow/ny.jpg" alt="New York">
                </div>
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

</div>


<div class="container marketing mt-2">
    @{ var confs = ViewData["confs"] as List<Conference>; }
    <div class="row">
        @foreach (var item in confs)
        {
            <div class="col-lg-4 col-md-6 text-center">
                <img class="img-circle" src="data:image/gif;base64,R0lGODlhAQABAIAAAHd3dwAAACH5BAAAAAAALAAAAAABAAEAAAICRAEAOw==" alt="Generic placeholder image" width="140" height="140">
                <h3>
                    <a href="/Conference/@item.FromDateTime.Year/@item.FromDateTime.Month/@item.FromDateTime.Day/@item.Slug" class="">
                        @item.Title
                    </a>
                </h3>
                <div style="font-size: 12px; height: 115px; overflow: hidden;">
                    @Html.Raw(item.Description)
